<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.webinfodesigner.conversorapp"
    android:versionCode="1"
    android:versionName="Versão 1.0">


    <!--Versão de Compilação-->
    <uses-sdk
        android:maxSdkVersion="23"
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logocma"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MediaActivity" />
        <activity android:name=".IMCActivity" />
        <activity android:name=".CalcActivity" />
        <activity android:name=".TempActivity" />
        <activity android:name=".HomeActivity" />
    </application>

</manifest>